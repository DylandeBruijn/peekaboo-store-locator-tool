{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "pslt/store-locator",
    "version": "1.0.0",
    "title": "Store Locator",
    "category": "widgets",
    "icon": "location-alt",
    "description": "A custom block that displays a store locator map.",
    "keywords": ["store", "locator", "map", "location"],
    "supports": {
        "align": true,
        "html": false,
        "jsx": true
    },
    "attributes": {
        "width": {
            "type": "string",
            "default": "100%"
        },
        "height": {
            "type": "string",
            "default": "500px"
        },
        "zoom": {
            "type": "number",
            "default": 12
        }
    },
    "textdomain": "pslt",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style.css",
    "viewScript": "file:./view.js",
    "render": "file:./render.php"
} 